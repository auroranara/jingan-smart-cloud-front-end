{
  "name": "ant-design-pro",
  "version": "2.0.0",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "presite": "npm run generate-mock && cd functions && npm install",
    "start": "cross-env ESLINT=none umi dev",
    "start:jiangxi": "cross-env PROJECT_ENV=jiangxi ESLINT=none umi dev",
    "start:shanxi": "cross-env PROJECT_ENV=shanxi ESLINT=none umi dev",
    "start:eye": "cross-env PROJECT_ENV=eye ESLINT=none umi dev",
    "start:no-mock": "cross-env MOCK=none ESLINT=none umi dev",
    "build": "cross-env ESLINT=none umi build",
    "build:default": "cross-env ESLINT=none umi build",
    "build:jiangxi": "cross-env PROJECT_ENV=jiangxi ESLINT=none umi build",
    "build:xuzhou": "cross-env PROJECT_ENV=xuzhou ESLINT=none umi build",
    "build:changshu": "cross-env PROJECT_ENV=changshu ESLINT=none umi build",
    "build:eye": "cross-env PROJECT_ENV=eye ESLINT=none umi build",
    "build:xuexiao": "cross-env PROJECT_ENV=xuexiao ESLINT=none umi build",
    "build:nanxiao": "cross-env PROJECT_ENV=nanxiao ESLINT=none umi build",
    "build:shanxi": "cross-env PROJECT_ENV=shanxi ESLINT=none umi build",
    "build:yanshi": "cross-env PROJECT_ENV=yanshi ESLINT=none umi build",
    "site": "npm run presite && npm run build && firebase deploy",
    "analyze": "cross-env ESLINT=none ANALYZE=1 umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "umi test",
    "generate-mock": "node ./scripts/generateMock.js",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@antv/data-set": "^0.9.6",
    "@babel/runtime": "^7.1.2",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "antd": "^3.11.6",
    "bizcharts": "^3.2.2",
    "bizcharts-plugin-slider": "^2.0.3",
    "braft-editor": "^2.1.28",
    "classnames": "^2.2.6",
    "coordtransform": "^2.1.2",
    "crypto-js": "^3.1.9-1",
    "dva": "^2.4.0",
    "dva-loading": "^2.0.3",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.14",
    "enquire-js": "^0.2.1",
    "hls.js": "^0.11.0",
    "js-file-download": "^0.4.3",
    "lodash-decorators": "^6.0.0",
    "lodash.isequal": "^4.5.0",
    "memoize-one": "^4.0.0",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.2.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "rc-animate": "^2.4.4",
    "rc-select": "^8.2.0",
    "react": "^16.6.3",
    "react-amap": "^1.2.7",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.6.3",
    "react-draggable": "^3.0.5",
    "react-fittext": "^1.0.0",
    "react-images": "^0.5.19",
    "react-infinite-scroller": "^1.2.1",
    "react-loadable": "^5.4.0",
    "react-pdf": "^3.0.5",
    "react-visibility-sensor": "^3.11.0",
    "rollbar": "^2.4.2",
    "setprototypeof": "^1.1.0",
    "url-polyfill": "^1.0.13",
    "video-react": "^0.13.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.53",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "antd-pro-merge-less": "^0.0.3",
    "antd-theme-webpack-plugin": "^1.0.8",
    "babel-eslint": "^8.2.6",
    "babel-runtime": "^6.9.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-umi": "^0.1.4",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "husky": "^1.0.0-rc.8",
    "lint-staged": "^7.2.0",
    "merge-umi-mock-data": "^0.0.3",
    "mockjs": "^1.0.1-beta3",
    "prettier": "1.14.3",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.12.0",
    "stylelint": "^9.4.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "umi": "^2.1.2",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-react": "^1.1.1",
    "web-report": "^1.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.6.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
